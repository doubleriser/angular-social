    "article-parser": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.16.1",
    "bootstrap": "3.3.7",
    "core-js": "^2.5.1",
    "cors": "^2.8.2",
    "email-verification": "latest",
    "express": "4.14.1",
    "express-limiter": "^1.6.1",
    "express-validator": "^3.1.2",
    "font-awesome": "4.7.0",
