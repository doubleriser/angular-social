  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "postinstall": "node install-env.js",
    "start": "node dist/server/app.js",
    "predev": "tsc -p ./server",
    "prod": "concurrently \"ng build -aot -prod\" \"node --harmony src/server/app.js\"",
    "dev": "concurrently  \"tsc -w -p  ./server\" \"ng serve -pc proxy.conf.json\" ",
    "servDev": "tsc -w -p  ./server&& nodemon dist/server/app.js",
    "startNodemon": "tsc -p ./server & node %NODE_DEBUG_OPTION%  dist/server/app.js",
    "buildDev": "ng serve -pc proxy.conf.json",
    "dev2": "npm run startNodemon & npm run dev",
    "debug": "concurrently \"nodemon %NODE_DEBUG_OPTION% dist/server/app.js  \"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
